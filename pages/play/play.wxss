.navbar {
  position: fixed;
  width: 100%;
}
.wraper {
  color: #fff;
}
.wraper .bgc {
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 0;
  top: 0;
  z-index: -99;
}
.wraper .bgc image {
  position: relative;
  left: -7%;
  top: -5%;
  width: 115%;
  height: 115%;
  filter: blur(40rpx);
}
.wraper .bgc .finish {
  width: 0;
  height: 0;
  opacity: 0;
}
.wraper .bgc .finish.finished {
  position: relative;
  left: -7%;
  top: -5%;
  width: 115%;
  height: 115%;
  opacity: 1;
}
.wraper .containt {
  width: 100%;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.3);
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.wraper .containt .header {
  padding-bottom: 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.wraper .containt .header .h3 {
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  font-size: 34rpx;
  font-weight: bold;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.wraper .containt .header .p {
  text-align: center;
  font-size: 24rpx;
}
.wraper .containt .main {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.wraper .containt .main .lyric-box {
  width: 80%;
  height: 100%;
  overflow: hidden;
  position: relative;
  transition: opacity 0.8s;
}
.wraper .containt .main .lyric-box .lyric {
  position: absolute;
  width: 100%;
  left: 0;
  top: calc(50% - 38rpx);
  transition: all 0.5s;
}
.wraper .containt .main .lyric-box .lyric .lyric-item {
  text-align: center;
  padding: 18rpx 0;
  font-size: 30rpx;
}
.wraper .containt .main .lyric-box .lyric .lyric-item.active {
  color: #06e6e6;
}
.wraper .containt .main .lyric-box .noLyric {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.wraper .containt .main .cover {
  position: absolute;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.wraper .containt .main .cover .animation1 {
  width: 500rpx;
  height: 500rpx;
  border-radius: 50%;
  overflow: hidden;
  animation: rotation1 30s linear infinite;
  transition: opacity 0.8s;
}
.wraper .containt .main .cover .animation1 image {
  width: 100%;
  height: 100%;
}
.wraper .containt .main .cover .animation1 .finish {
  width: 0;
  height: 0;
  opacity: 0;
}
.wraper .containt .main .cover .animation1 .finish.finished {
  width: 100%;
  height: 100%;
  opacity: 1;
}
.wraper .containt .main .cover .animation2 {
  width: 500rpx;
  height: 500rpx;
  border-radius: 50%;
  overflow: hidden;
  animation: rotation2 30s linear infinite;
  transition: opacity 0.8s;
}
.wraper .containt .main .cover .animation2 image {
  width: 100%;
  height: 100%;
}
.wraper .containt .main .cover .animation2 .finish {
  width: 0;
  height: 0;
  opacity: 0;
}
.wraper .containt .main .cover .animation2 .finish.finished {
  width: 100%;
  height: 100%;
  opacity: 1;
}
@keyframes rotation1 {
  from {
    -webkit-transform: rotate(0deg);
  }
  to {
    -webkit-transform: rotate(360deg);
  }
}
@keyframes rotation2 {
  from {
    -webkit-transform: rotate(0deg);
  }
  to {
    -webkit-transform: rotate(360deg);
  }
}
.wraper .containt .footer {
  width: 100%;
  padding: 40rpx 0 20rpx;
}
.wraper .containt .footer .progress {
  display: flex;
  align-items: center;
  width: 90%;
  margin: 0 auto;
}
.wraper .containt .footer .progress .slider {
  flex: 1;
  margin: 0;
  padding: 5rpx 10rpx;
}
.wraper .containt .footer .progress .currentTime,
.wraper .containt .footer .progress .totalTime {
  width: 64rpx;
  flex-shrink: 0;
  font-size: 20rpx;
  line-height: 20rpx;
  font-weight: 100;
  color: rgba(255, 255, 255, 0.5);
  text-align: center;
}
.wraper .containt .footer .icon {
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.wraper .containt .footer .icon .iconfont {
  font-size: 54rpx;
  padding: 10rpx;
}
.wraper .containt .footer .icon .iconfont.icon-zanting,
.wraper .containt .footer .icon .iconfont.icon-bofang {
  font-size: 140rpx;
  padding: 0;
}
