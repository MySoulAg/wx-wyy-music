.navbar {
  position: fixed;
  width: 100%;
  z-index: 9999;
}
.wraper .bgc {
  width: 100vw;
  height: 400rpx;
  position: fixed;
  left: 0;
  top: 0;
  z-index: -99;
}
.wraper .bgc image {
  position: relative;
  left: -7%;
  top: -5%;
  width: 115%;
  height: 115%;
  filter: blur(30rpx);
}
.wraper .albumInfo {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  box-sizing: border-box;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-bottom: 100rpx;
  display: flex;
  background-color: rgba(0, 0, 0, 0.3);
}
.wraper .albumInfo .image-loader {
  width: 230rpx;
  height: 230rpx;
}
.wraper .albumInfo .left {
  flex: 1;
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.wraper .albumInfo .left .title {
  font-size: 32rpx;
  font-weight: bold;
  color: #fff;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.wraper .albumInfo .left .mid {
  display: flex;
  align-items: center;
}
.wraper .albumInfo .left .mid .image-loader {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  overflow: hidden;
}
.wraper .albumInfo .left .mid .name {
  flex: 1;
  font-size: 24rpx;
  color: #eee;
  margin-left: 10rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.wraper .albumInfo .left .describe {
  font-size: 24rpx;
  color: #eee;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.wraper .songList {
  border-radius: 30rpx 30rpx 0 0;
  position: fixed;
  left: 0;
  width: 100%;
  background-color: #fff;
}
.wraper .songList .title {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}
.wraper .songList .title .icon-zanting {
  width: 80rpx;
  font-size: 44rpx;
  font-weight: bold;
  text-align: center;
}
.wraper .songList .title .text {
  font-size: 36rpx;
  font-weight: bold;
}
.wraper .songList .title .allNum {
  font-size: 26rpx;
  color: #999;
}
.wraper .songList .list .item {
  display: flex;
  align-items: center;
  padding: 15rpx 30rpx 15rpx 0;
}
.wraper .songList .list .item .num {
  width: 80rpx;
  text-align: center;
  font-size: 32rpx;
  color: #999;
  flex-shrink: 0;
}
.wraper .songList .list .item .left {
  width: calc(100% - 80rpx);
  display: flex;
  flex-direction: column;
}
.wraper .songList .list .item .left .name {
  font-size: 36rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.wraper .songList .list .item .left .name text {
  color: #999;
}
.wraper .songList .list .item .left .author {
  font-size: 26rpx;
  color: #999;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.playing-circle {
  position: fixed;
  right: 40rpx;
}
